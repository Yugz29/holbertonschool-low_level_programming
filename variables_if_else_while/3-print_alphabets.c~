#include <stdio.h>

/**
 * main - print in lowercase and uppercase
 *
 * Return : always return 0
 *
 **/

int main(void)
{
	char lettre = 'a';
	char LETTRE = 'A';

	while (lettre <= 'z')
	{
		putchar(lettre);
		lettre++;
	}
	while (LETTRE <= 'Z')
	{
		putchar(LETTRE);
		LETTRE++;
	}
	putchar('\n');

	return (0);

}
