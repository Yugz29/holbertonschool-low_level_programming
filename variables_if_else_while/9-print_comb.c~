#include <stdio.h>

/**
 * main - print all possible combinations of single-digit numbers
 *
 * Return: always return 0
 *
 **/

int main(void)
{
	int i = 0;

	while (i <= 9)
	{
		putchar(i + 48);

		if (i < 9)
		{
			putchar(',');
			putchar(' ');
		}
		else
		{
			putchar('\n');
		}
		
		i++;
	}

	return (0);
}
