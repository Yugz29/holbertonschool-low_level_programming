#include "main.h"
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

/**
 *
 *
 *
 *
 *
 *
 **/

void *_calloc(unsigned int nmemb, unsigned int size)
{
	void *ptr;
	char *cptr;
	unsigned int i;

	if (nmemb == 0 || size == 0)
	{
		return (NULL);
	}

	ptr = malloc(nmemb * size);

	if (ptr == 0)
	{
		return (NULL);
	}

	cptr = (char *)ptr;

	for (i = 0; i < nmemb * size; i++)
	{
		cptr[i] = 0;
	}

	return (ptr);
}
